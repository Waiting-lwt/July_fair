radio .uni-radio-input{
	-webkit-transform:scale(0.6);
	        transform:scale(0.6);
}
radio.free .uni-radio-input.uni-radio-input-checked{
	background: #041746 !important;
	border-color: #041746 !important;
}
radio.fast .uni-radio-input.uni-radio-input-checked{
	background: #f05050 !important;
	border-color: #ff0000 !important;
}
radio .uni-radio-input.uni-radio-input-checked::before{
	font-size: 0rpx; /* 对勾大小 去掉 */
}

.arrivalNavigation {
	width: 250rpx;
	position: absolute;
	left: 20rpx;
	top: 50rpx;
	z-index: 99;
}
.arrivalNavigation .sideNavigation {
	width: 248rpx;
	background-color: #e8f0ff;
	color: #eee;
	border-radius: 10rpx;
}
.sideNavigation ._li {
	height: 85rpx;
	text-align: center;
	line-height: 85rpx;
	font-size: 25rpx;
}
.sideNavigation .liBottomBorder {
	border: 0.1rpx solid #b6b6b6;
}
.sideNavigation .navigatorStyle {
	color: black;
}
.arrivalNavigation .d4 {
	/* position: absolute;
 */
	/*  left: 140rpx;
 */
	width: 0;
	height: 0;
	margin-left: 150rpx;
	margin-top: -20rpx;
	border-width: 20rpx;
	border-style: solid;
	border-color: transparent #e8f0ff transparent transparent;
	-webkit-transform: rotate(90deg);
	        transform: rotate(90deg);
	/*顺时针旋转90°*/
}

